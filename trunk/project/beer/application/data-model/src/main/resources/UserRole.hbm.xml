<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_SMARTORG_ROLE" name="com.apusic.ebiz.model.user.Role">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_smartorg_role</param>
      </generator>
    </id>
    <property name="name" length="50" column="F_ROLE_NAME" access="field" type="string"/>
    <property name="alias" length="50" column="F_ROLE_ALIAS" access="field" type="string"/>
    <property name="desc" length="50" column="F_ROLE_DESC" access="field" type="string"/>
    <set cascade="none" access="field" lazy="true" table="T_SMARTORG_ROLE_RESOURCE" name="resources">
      <key column="F_ROLE_ID"/>
      <many-to-many class="com.apusic.ebiz.model.user.Resource" column="F_RESOURCE_ID"/>
    </set>
  </class>
</hibernate-mapping>
