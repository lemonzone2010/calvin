<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_FAVORITE" name="com.apusic.md.model.emarket.Favorite">
    <id name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_favorite</param>
      </generator>
    </id>
    <property name="memberId"  column="F_MEMBER_ID" type="integer"/>
    <property name="productId"  column="F_PRODUCT_ID"  type="integer"/>
    <property name="productName"  column="F_PRODUCT_NAME" type="string"/>
    <property name="wholesalePrice"  column="F_WHOLESALE_PRICE" type="double"/>
    <property name="retailPrice"  column="F_RETAIL_PRICE" type="double"/>
    <property name="productImg"  column="F_PRODUCT_IMG"  type="string"/>
    <property name="createTime"  column="F_CREATE_TIME"  type="date"/>
    <property name="saleType"  column="F_SALE_TYPE"  type="com.apusic.md.model.emarket.SaleEnumType"/>
  </class>
</hibernate-mapping>
