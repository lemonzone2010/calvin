<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_USERSPHERE_RETRIEVEPW" name="com.apusic.md.model.usersphere.RetrievePW">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_usersphere_retrievepw</param>
      </generator>
    </id>
    <property name="name" length="50" column="F_NAME" unique="true" access="field" type="string" not-null="true"/>
    <property name="email" length="50" column="F_EMAIL" access="field" type="string" not-null="true"/>
    <property name="securityCode" length="20" column="F_SECURITY_CODE" access="field" type="string" not-null="true"/>
    <property name="resetCode" length="100" column="F_RESET_CODE" access="field" type="string" not-null="true"/>
    <property name="timeout" column="F_TIMEOUT" access="field" type="timestamp" not-null="true"/>
  </class>
</hibernate-mapping>
