<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_HELP_CENTER_CONTENT" name="com.apusic.md.model.emarket.HelpCenterContent">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_help_content</param>
      </generator>
    </id>
    <property name="titleId" column="F_TITLE_ID" type="integer"/>
    <property name="content" column="F_CONTENT" type="string"/>
  </class>
</hibernate-mapping>