<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_ADVERTISEMENT" name="com.apusic.md.model.emarket.Advertisement">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_advertisement</param>
      </generator>
    </id>
    <property name="name" length="100" column="F_NAME" access="field" type="string"/>
    <property name="imagePath" length="50" column="F_IMAGE_PATH" access="field" type="string"/>
    <property name="url" length="50" column="F_URL" access="field" type="string"/>
    <property name="serialNumber" column="F_SERIAL_NUMBER" access="field" type="integer"/>
    <many-to-one  name="position" class="com.apusic.md.model.emarket.AdvertisementPosition" column="F_POSITION_ID"/>
  </class>
</hibernate-mapping>
