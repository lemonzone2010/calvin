<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_PRODUCT_TEMPLATE" name="com.apusic.md.model.emarket.ProductTemplate">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_product_template</param>
      </generator>
    </id>
    <property name="templateName" column="F_PRODUCT_TEMPLATE_NAME" access="field" type="string" length="200"/>
    <property name="categoryId" column="F_CATEGORY_ID" access="field" type="integer"/>
    <property name="brandId" column="F_BRAND_ID" access="field" type="integer"/>
    <property name="wholesalePrice" column="F_WHOLESALE_PRICE" access="field" type="double"/>
    <property name="retailPrice" column="F_RETAIL_PRICE" access="field" type="double"/>
    <property name="code" length="50" column="F_CODE" access="field" type="string"/>
    <property name="createTime" column="F_CREATE_TIME" access="field" type="timestamp"/>
    <property name="keywords" length="200" column="F_KEYWORDS" access="field" type="string"/>
    <property name="desc" length="300" column="F_DESCRIPTION" access="field" type="string"/>
    <property name="introduction" length="4000" column="F_INTRODUCTION" access="field" type="string"/>
    <property name="stock" length="50" column="F_STOCK" access="field" type="com.apusic.md.model.emarket.ProductStockEnumType"/>
    <property name="customization" length="2000" column="F_CUSTOMIZATION" access="field" type="string"/>
    <property name="attribute" length="4000" column="F_ATTRIBUTE" access="field" type="string"/>
    <property name="wholeSaleThrehold" column="F_WHOLESALE_THRESHOLD" access="field" type="integer"/>
    <property name="saleType" column="F_IS_WHOLESALE" length="50" access="field" type="com.apusic.md.model.emarket.SaleEnumType"/>
    <property name="creditRaito" column="F_CREDIT_RATIO" access="field" type="float"/>

	<many-to-one  name="user" class="com.apusic.ebiz.model.user.User" not-null="true" column="F_USER_ID"/>
  </class>
</hibernate-mapping>
