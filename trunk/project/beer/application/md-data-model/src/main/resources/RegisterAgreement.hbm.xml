<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_USERSPHERE_AGREEMENT" name="com.apusic.md.model.usersphere.RegisterAgreement">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_usersphere_agreement</param>
      </generator>
    </id>
    <property name="type" length="25" column="F_AGREEMENT_NAME" access="field" unique="true" type="com.apusic.md.model.usersphere.AgreementEnumType"/>
    <property name="agreementContent" length="50" column="F_AGREEMENT_CONENT" access="field" not-null="true" type="string"/>
  </class>
</hibernate-mapping>
