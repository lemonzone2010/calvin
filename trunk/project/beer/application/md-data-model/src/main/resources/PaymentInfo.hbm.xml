<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_ORDERBOOK_PAYMENT_INFO" name="com.apusic.md.model.orderbook.PaymentInfo">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_orderbook_payment_info</param>
      </generator>
    </id>
    <many-to-one name="order" class="com.apusic.md.model.orderbook.Order" unique="true" column="F_ORDER_ID"/>
    <property name="payMode" column="F_PAY_MODE" type="com.apusic.md.model.orderbook.PayModeEnumType"/>
    <property name="amount" column="F_AMOUNT"  type="double"/>
    <property name="payType" column="F_PAY_TYPE"  type="com.apusic.md.model.orderbook.PayTypeEnumType"/>
  </class>
</hibernate-mapping>
