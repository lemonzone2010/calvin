<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_ORDERBOOK_ORDER" name="com.apusic.md.model.orderbook.Order">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_orderbook_order</param>
      </generator>
    </id>
    <property name="number" column="F_NUMBER"  type="string"/>
    <property name="state" column="F_STATE" type="com.apusic.md.model.orderbook.OrderStateEnumType"/>
    <property name="createTime" column="F_CREATE_TIME"  type="timestamp"/>
    <property name="creator" column="F_CREATOR"  type="string"/>
    <property name="seller" column="F_SELLER"  type="string"/>
    <property name="totalPrice" column="F_TOTAL_PRICE" type="double"/>
    <property name="saleType" column="F_SALE_TYPE"  type="com.apusic.md.model.emarket.SaleEnumType"/>
    <property name="remark" column="F_REMARK" type="string" />
    <property name="workflowId" column="F_WORKFLOW_ID"  type="long"/>
    <property name="appraise" column="F_APPRAISE" type="boolean" />
    <one-to-one name="deliveryInfo" class="com.apusic.md.model.orderbook.DeliveryInfo" property-ref="order" cascade="all"/>
    <one-to-one name="paymentInfo" class="com.apusic.md.model.orderbook.PaymentInfo" property-ref="order" cascade="all"/>
    <one-to-one name="orderInvoice" class="com.apusic.md.model.orderbook.OrderInvoice" property-ref="order" cascade="all"/>
    <set name="orderItems" inverse="true" cascade="all" lazy="true">
		<key column="F_ORDER_ID"/>
		<one-to-many class="com.apusic.md.model.orderbook.OrderItem"/>
	</set>
  </class>
</hibernate-mapping>
