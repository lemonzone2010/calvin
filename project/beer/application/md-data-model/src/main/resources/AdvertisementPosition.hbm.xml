<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_AD_POSITION" name="com.apusic.md.model.emarket.AdvertisementPosition">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_ad_position</param>
      </generator>
    </id>
    <property name="position" length="50" column="F_POSITION" access="field" type="string"/>
    <property name="desc" length="1000" column="F_DESC" access="field" type="string"/>
    <property name="limit" column="F_LIMIT" access="field" type="integer"/>
	<set name="advertisements" cascade="all" inverse="true">
		<key column="F_POSITION_ID"/>
		<one-to-many class="com.apusic.md.model.emarket.Advertisement"/>
	</set>
  </class>
</hibernate-mapping>
