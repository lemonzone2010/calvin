<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_NEWS" name="com.apusic.md.model.emarket.News">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_news</param>
      </generator>
    </id>
    <property name="name" length="50" column="F_NAME" access="field" type="string"/>
    <property name="createTime" column="F_CREATE_TIME" access="field" type="timestamp"/>
    <property name="content" column="F_CONTENT" access="field" type="string"/>
  </class>
</hibernate-mapping>
