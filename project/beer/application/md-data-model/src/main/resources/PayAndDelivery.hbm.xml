<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_ORDERBOOK_PAY_DELIVERY" name="com.apusic.md.model.orderbook.PayAndDelivery">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_orderbook_pay_delivery</param>
      </generator>
    </id>

    <property name="payMode" column="F_PAY_MODE" type="com.apusic.md.model.orderbook.PayModeEnumType"/>
    <property name="deliveryMode" column="F_DELIVERY_MODE" type="com.apusic.md.model.orderbook.DeliveryModeEnumType"/>
    <property name="deliveryTime" column="F_DELIVERY_TIME" type="com.apusic.md.model.orderbook.DeliveryTimeSlotEnumType"/>
    <property name="userName" column="F_USER_NAME" type="string"/>
  </class>
</hibernate-mapping>
