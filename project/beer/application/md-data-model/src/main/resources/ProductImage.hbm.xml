<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_PRODUCT_IMAGE" name="com.apusic.md.model.emarket.ProductImage">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_product_image</param>
      </generator>
    </id>
    <property name="imagePath" length="100" column="F_IMAGE_PATH" access="field" type="string"/>
    <property name="serialNumber" column="F_SERIAL_NUMBER" access="field" type="integer"/>
    <many-to-one  name="product" class="com.apusic.md.model.emarket.Product" column="F_PRODUCT_ID"/>
  </class>
</hibernate-mapping>
