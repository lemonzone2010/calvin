<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_PRODUCT_RECOMMEND" name="com.apusic.md.model.emarket.ProductRecommend">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_product_recommend</param>
      </generator>
    </id>
    <many-to-one  name="product" class="com.apusic.md.model.emarket.Product" access="field"  column="F_PRODUCT_ID"/>
    <property name="serialNumber"  column="F_SERIAL_NUMBER" access="field" type="integer"/>
    <property name="startDate"  column="F_START_TIME" access="field" type="date"/>
    <property name="endDate"  column="F_END_TIME" access="field" type="date"/>
    <property name="type"  column="F_TYPE" access="field" type="com.apusic.md.model.emarket.RecommendEnumType"/>
  </class>
</hibernate-mapping>
