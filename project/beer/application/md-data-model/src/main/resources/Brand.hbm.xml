<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_EMARKET_BRAND" name="com.apusic.md.model.emarket.Brand">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_emarket_brand</param>
      </generator>
    </id>

    <property name="name" length="100" column="F_NAME" access="field" type="string"/>
    <property name="logoPath" length="100" column="F_LOGO_PATH" access="field" type="string"/>
    <property name="state" length="50" column="F_STATE" access="field" type="com.apusic.md.model.emarket.StateEnumType"/>
    <property name="url" length="50" column="F_URL" access="field" type="string"/>

    <set cascade="none" access="field" lazy="true" table="T_EMARKET_BRAND_CATEGORY" name="categorys">
      <key column="F_BARND_ID"/>
      <many-to-many class="com.apusic.md.model.emarket.ProductCategory" column="F_CATEGORY_ID"/>
    </set>
  </class>
</hibernate-mapping>
