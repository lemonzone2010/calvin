<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_ORDERBOOK_ORDER_INVOICE" name="com.apusic.md.model.orderbook.OrderInvoice">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_orderbook_order_invoice</param>
      </generator>
    </id>
    <many-to-one name="order" class="com.apusic.md.model.orderbook.Order" unique="true" column="F_ORDER_ID"/>
    <property name="type" column="F_TYPE" type="com.apusic.md.model.orderbook.InvoiceTypeEnumType"/>
    <property name="head" column="F_HEAD"  type="string"/>
    <property name="content" column="F_CONTENT"  type="com.apusic.md.model.orderbook.InvoiceContentTypeEnumType"/>
  </class>
</hibernate-mapping>
