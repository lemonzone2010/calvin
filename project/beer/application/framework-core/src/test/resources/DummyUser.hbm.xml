<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.apusic.ebiz.framework.core">
	<class table="t_user" name="DummyUser">
	    <id name="id">
	      <column name="ID"/>
	      <generator class="native">
	        <param name="sequence">seq_md_user</param>
	      </generator>
	    </id>
	    <property name="name" length="50" column="name" type="string"/>
	    <property name="sex" length="10" column="sex" type="string"/>
	    <property name="password" length="50" column="password" not-null="true" type="string"/>
	    <property name="createTime" column="createTime" type="timestamp"/>
	    <many-to-one column="roleid" class="DummyRole" name="role" cascade="persist"/>
	    <property name="userType" length="40" column="F_USER_TYPE" access="field" type="com.apusic.ebiz.framework.core.UserEnumType"></property>
  	</class>

	<class name="Permission" table="T_PERMISSION_TEST">
		<id column="F_ID" name="id" type="java.lang.Long">
			<generator class="native">
	        <param name="sequence">seq_md_per</param>
	      </generator>
		</id>
		<property column="F_NAME" name="name" type="java.lang.String" />
	</class>

</hibernate-mapping>