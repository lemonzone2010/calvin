<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_NAVIGATION_APPLICATION" name="com.apusic.ebiz.model.navigation.ApplicationInfo">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_navigation_application</param>
      </generator>
    </id>
    <property name="status" length="10" column="F_STATUS" type="string"/>
    <property name="applicationRoot" column="F_APPLICATION_ROOT" length="30" type="string"/>
    <property name="applicationName" column="F_APPLICATION_NAME" length="30" type="string"/>
    <property name="sequence" column="F_SEQUENCE" length="3" type="int"/>
  </class>
</hibernate-mapping>
