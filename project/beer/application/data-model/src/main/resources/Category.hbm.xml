<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="T_DATAMANAGEMENT_CATEGORY" name="com.apusic.ebiz.model.datamanagement.Category">
    <id access="field" name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_datamanagement_category</param>
      </generator>
    </id>
    <property name="name" length="50" column="F_CATEGORY_NAME" not-null="true" access="field" type="string"/>
    <property name="intro" length="200" column="F_CATEGORY_INTRO" not-null="true" access="field" type="string"/>
    <set name="datas" access="field" lazy="true" table="T_DATAMANAGEMENT_DATA">
    	<key column="F_DATA_CATEGORY"/>
    	<one-to-many class="com.apusic.ebiz.model.datamanagement.ConfigurationData"/>
    </set>
  </class>
</hibernate-mapping>
