<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="true" package="com.xia.search.solr.dao">
  <class table="T_DUMMY_BOOK" name="DummyBook">
    <id name="id">
      <column name="F_ID"/>
      <generator class="native">
        <param name="sequence">seq_md_dummy_book</param>
      </generator>
    </id>
    <property name="publicationDate" column="F_PUBLICATION_DATE" type="date" />
     <property name="subtitle" length="500" column="F_SUBTITLE" type="string"/>
    <property name="title" length="500" column="F_TITLE" type="string"/>
    <property name="price" column="F_RETAIL_PRICE" access="field" type="double"/>
  </class>
</hibernate-mapping>
